{
  "name": "xiyongApplication",
  "author": "RexHang",
  "create": "2019",
  "version": "0.0.1",
  "homepage": ".",
  "main": "main.js",
  "DEV": false,
  "private": true,
  "build": {
    "appId": "com.xiyongzy.app"
  },
  "dependencies": {
    "@babel/core": "7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.2",
    "antd": "^3.18.1",
    "awesome-typescript-loader": "^5.2.1",
    "axios": "^0.18.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "9.0.0",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.4",
    "babel-plugin-import": "^1.11.0",
    "babel-plugin-named-asset-import": "^0.2.2",
    "babel-preset-react-app": "^5.0.4",
    "bfj": "6.1.1",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "chalk": "2.4.1",
    "css-loader": "1.0.0",
    "dotenv": "6.0.0",
    "dotenv-expand": "4.2.0",
    "echarts": "^4.1.0",
    "echarts-for-react": "^2.0.15-beta.0",
    "eslint": "5.6.0",
    "eslint-config-react-app": "^3.0.4",
    "eslint-loader": "2.1.1",
    "eslint-plugin-flowtype": "2.50.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.11.1",
    "file-loader": "2.0.0",
    "fs-extra": "7.0.0",
    "fundebug-javascript": "^1.7.3",
    "html-webpack-plugin": "4.0.0-alpha.2",
    "identity-obj-proxy": "3.0.0",
    "jest": "23.6.0",
    "jest-pnp-resolver": "1.0.1",
    "jest-resolve": "23.6.0",
    "jsonp": "^0.2.1",
    "less": "^2.7.3",
    "less-loader": "^4.1.0",
    "lodash": "^4.17.11",
    "mini-css-extract-plugin": "0.4.3",
    "moment": "latest",
    "myloggerjs": "^1.0.4",
    "node-sass": "^4.9.4",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "pnp-webpack-plugin": "1.1.0",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.0.6",
    "postcss-safe-parser": "4.0.1",
    "prop-types": "^15.7.2",
    "pubsub-js": "^1.7.0",
    "react": "^16.8.0-alpha.0",
    "react-app-polyfill": "^0.1.3",
    "react-dev-utils": "^6.0.5",
    "react-dom": "^16.8.0-alpha.0",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "reqwest": "^2.0.5",
    "resolve": "1.8.1",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.0",
    "terser-webpack-plugin": "1.1.0",
    "url-loader": "1.1.1",
    "vconsole": "^3.2.0",
    "wangeditor": "^3.1.1",
    "webpack": "4.19.1",
    "webpack-dev-server": "3.1.9",
    "webpack-manifest-plugin": "2.0.4",
    "workbox-webpack-plugin": "3.6.2"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "https": "set HTTPS=true&&node scripts/start.js",
    "https2": "($env:HTTPS = \"true\") -and (node scripts/start.js)",
    "https3": "HTTPS=true node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js",
    "electron-start": "electron .",
    "pack": "electron-packager . xiyongApplication --win --out . --arch=x64 --app-version=0.0.1 --overwrite --electron-version=6.0.9 --icon=./public/favicon.ico --ignore=\"(src|.idea|.git|.eslint*|yarn-error.log|yarn.lock|docs|.gitignore|LICENSE|README.md|webpack.config*|node_modules)\"",
    "packformacos": "electron-packager . xiyongApplication --platform=darwin --out . --arch=x64 --app-version=0.0.1 -overwrite --electron-version=6.0.9 --icon=app.icns --asar --ignore=\"(src|.idea|.git|.eslint*|yarn-error.log|yarn.lock|docs|.gitignore|LICENSE|README.md|webpack.config*|node_modules)\"",
    "asar": "asar pack ./app app.asar"
  },
  "eslintConfig": {},
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ],
      [
        "import",
        {
          "libraryName": "antd",
          "libraryDirectory": "es",
          "style": true
        }
      ]
    ]
  },
  "devDependencies": {
    "electron": "^6.0.9",
    "electron-packager": "^14.1.1"
  }
}
