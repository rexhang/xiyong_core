/**
 * @author Rexhang(GuHang)
 * @date  2018
 * @Description: Team development configuration
*/
{
	"extends": "react-app",
	"rules": {
		"no-unreachable": [
			0
		],
		"no-undef": [
			0
		],
		"indent": [
			2,
			"tab"
		],
		"curly": [
			2,
			"all"
		],
		"array-bracket-spacing": [
			2,
			"never"
		],
		"no-var": [
			2
		],
		"no-this-before-super": [
			2
		],
		"no-trailing-spaces": [
			1
		],
		"no-multiple-empty-lines": [
			2,
			{"max": 1}
		],
		"no-multi-spaces": [
			2
		],
		"no-irregular-whitespace": [
			2
		],

		"no-extra-parens": [
			2
		],
		"no-else-return": [
			1
		],
		"no-dupe-keys": [
			2
		],
		"no-const-assign": [
			2
		],
		"consistent-this": [
			2,
			"vm"
		],
		"eqeqeq": [
			2,
			"always"
		],
		"semi": [
			2,
			"always"
		],
		"semi-spacing": [
			2,
			{
				"before": false,
				"after": true
			}
		],
		"semi-style": [
			2,
			"last"
		],
		"no-unused-vars": [
			1
		],
		"no-unused-expressions": [
			0
		],
		"no-lone-blocks": [
			0
		]
	}
}